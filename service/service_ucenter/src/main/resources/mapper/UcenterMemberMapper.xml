<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.educenter.mapper.UcenterMemberMapper">

    <resultMap id="BaseResultMap" type="com.cn.educenter.domain.UcenterMember">
        <id property="id" column="id" jdbcType="CHAR"/>
        <result property="openid" column="openid" jdbcType="VARCHAR"/>
        <result property="mobile" column="mobile" jdbcType="VARCHAR"/>
        <result property="password" column="password" jdbcType="VARCHAR"/>
        <result property="nickname" column="nickname" jdbcType="VARCHAR"/>
        <result property="sex" column="sex" jdbcType="TINYINT"/>
        <result property="age" column="age" jdbcType="TINYINT"/>
        <result property="avatar" column="avatar" jdbcType="VARCHAR"/>
        <result property="sign" column="sign" jdbcType="VARCHAR"/>
        <result property="isDisabled" column="is_disabled" jdbcType="TINYINT"/>
        <result property="isDeleted" column="is_deleted" jdbcType="TINYINT"/>
        <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
        <result property="gmtModified" column="gmt_modified" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id
        ,openid,mobile,
        password,nickname,sex,
        age,avatar,sign,
        is_disabled,is_deleted,gmt_create,
        gmt_modified
    </sql>
    <select id="selectOneByOpenid" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from ucenter_member
        where
        openid = #{openid,jdbcType=VARCHAR}
    </select>
</mapper>
